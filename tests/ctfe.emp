# These examples are run via compile-time function evaluation.
# Enable --dump-vvm to see how the comptime literals have been elided.

func inc(x: Int64):
  return x + 1
end

let x = inc(7)
print(x)
##8

data Person:
  name: String,
  age: Int64
end

let p = Person("a", 123)
let y = p.age
print(y)
##123

let df_p = !Person(["a", "b"], [1, 2]).name[1]
print(df_p)
##b

let z = compile("x + y")
print(z)
##131

func foo(x: Int64):
  let temp1 = 1
  let temp2 = temp1 + 3
  return x + temp2
end

print(foo(7))
##11

