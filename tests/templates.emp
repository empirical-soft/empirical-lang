# Examples of templates. Enable --dump-vvm to see how everything is generated

func foo{x: Int64}(y: Int64):
  return x + y
end

# create a new function
print(foo{7}(8))
##15

# reuse the above function
print(foo{7}(9))
##16

# create another function
print(foo{8}(9))
##17

func baz{x: Int64, y: Int64}(z: Int64):
  var temp = x + y
  return temp + z
end

# multiple template parameters
print(baz{1, 2}(3))
##6

