# Tests for generic placeholders. Enable --dump-vvm to see generated assembly.

func foo[T](a: T, b: T):
  let c: T = a + b
  return c
end

print(foo(4, 5))
##9

print(foo(4., 5.))
##9.0

print(foo([1, 2], [3, 4]))
##[4, 6]


func foo2[T](a: [T], b: T) = a + b

print(foo2([1, 2, 3], 5))
##[6, 7, 8]


func foo3[T, U](a: T, b: U) = a + b

print(foo3(4, 5.))
##9.0

